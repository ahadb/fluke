{
  "extends": "./setup/.globals.json",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "strict": 0,
    "quotes": [2, "single"],
    "no-unused-expressions": 0,
    "indent": ["error", 2]
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  }
}
